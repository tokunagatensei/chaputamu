<div class="side_bar_header">
  <div class="header-name">
    <%= @room.room_name %>
  </div>
</div>

<div class="rooms">

  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.room_name, room_messages_path(room) %>
      </div>
    </div>
  <% end %>

</div>

<div class="room-back">
    <%= link_to '戻る', "/users/#{current_user.id}", class:'room-back-btn' %>
</div>